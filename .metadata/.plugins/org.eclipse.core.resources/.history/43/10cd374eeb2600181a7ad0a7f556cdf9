import java.util.Arrays;
import java.util.Scanner;
import static java.lang.System.*;
import static java.util.Arrays.*;

public class Grades
{
	private double[] grades;
	
	public Grades()
	{
		setGrades("");
	}
	
	public Grades(String gradeList)
	{
		setGrades(gradeList);		
	}
	
	public void setGrades(String gradeList)
	{
		Scanner keyboard= new Scanner (gradeList);
		int x= keyboard.nextInt();
		grades=new double[x];
		keyboard.next();
		for(int i=0; i<x;i++)
		{
			grades[i]=keyboard.nextDouble();
		}
	}
	
	public void setGrade(int spot, double grade)
	{
		grades[spot]=grade;
	}
	
	public double getSum()
	{
		double sum=0.0;
		for(int j=0;j<grades.length;j++)
		{
			sum=sum+grades[j];
		}
		return sum;
	}
	
	public double getLowGrade()
	{
		double low = Double.MAX_VALUE;
		for(int z=0;z<grades.length;z++)
		{
			if(grades[z]<low)
			{
				low=grades[z];
			}
		}
		return low;
	}
	
	public double getHighGrade()
	{
		double high = Double.MIN_VALUE;
		for(int z=0;z<grades.length;z++)
		{
			if(grades[z]>high)
			{
				high=grades[z];
			}
		}
		return high;
	}
	
	public int getNumGrades()
	{
		return grades.length;
	}
	
	public String toString()
	{
		//String output= grades + "\n" + "Sum = " + getSum() +"\n" + "num grades = " + getNumGrades() + "\n" + "low grade = " + getLowGrade() + "\n" + "high grade = " + getHighGrade() +"\n";
		String output=Arrays.toString(grades);




		return output;
	}	
}